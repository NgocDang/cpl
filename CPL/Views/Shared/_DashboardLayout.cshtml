<!DOCTYPE html>
<html class="loading" lang="en" data-textdirection="ltr">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <meta name="description" content="Crypto ICO admin is super flexible, powerful, clean &amp; modern responsive bootstrap 4 admin template with unlimited possibilities with bitcoin dashboard.">
    <meta name="keywords" content="admin template, crypto ico admin template, dashboard template, flat admin template, responsive admin template, web app, crypto dashboard, bitcoin dashboard">
    <meta name="author" content="PIXINVENT">
    <title>@ViewData["Title"] - @LangDetailHelper.Get(Context.Session.GetInt32("LangId").Value, "TokenSymbol")</title>
    <link href="https://fonts.googleapis.com/css?family=Muli:300,300i,400,400i,600,600i,700,700i|Comfortaa:300,400,500,700" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="~/css/dashboard/vendors.min.css">
    <link rel="stylesheet" type="text/css" href="~/css/dashboard/app.min.css">
    <link rel="stylesheet" type="text/css" href="~/css/dashboard/vertical-compact-menu.min.css">
    <link rel="stylesheet" type="text/css" href="~/css/dashboard/plugins/cryptocoins/cryptocoins.css">
    <link rel="stylesheet" type="text/css" href="~/css/dashboard/timeline.min.css">
    <link rel="stylesheet" type="text/css" href="~/css/dashboard/dashboard-ico.min.css">
    <link rel="stylesheet" type="text/css" href="~/css/dashboard/style.css">
    @RenderSection("styles", required: false)
</head>
<body class="vertical-layout vertical-compact-menu 2-columns   menu-expanded fixed-navbar" data-open="click" data-menu="vertical-compact-menu" data-col="2-columns">
    @await Component.InvokeAsync("DashboardNavbar")
    @await Component.InvokeAsync("DashboardSidebar")
    @RenderBody()
    @await Component.InvokeAsync("DashboardFooter")
    <div id="modal"></div>
    <script src="~/js/dashboard/vendors.min.js" type="text/javascript"></script>
    <script src="~/js/dashboard/horizontal-timeline.js" type="text/javascript"></script>
    <script src="~/js/dashboard/app-menu.min.js" type="text/javascript"></script>
    <script src="~/js/dashboard/app.min.js" type="text/javascript"></script>
    <script src="~/js/dashboard/demo.min.js" type="text/javascript"></script>
    @Html.Hidden("LangId", Context.Session.GetInt32("LangId").Value)
    @RenderSection("scripts", required: false)
</body>
</html>
